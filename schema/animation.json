{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
        "value": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "value": {
                        "type": "array",
                        "items": {
                            "type":"object",
                            "properties": {
                                "key": {
                                    "type": "string"
                                },
                                "value": {
                                    "type": ["number","string"]
                                }
                            },
                            "required": ["key","value"]
                        }
                    },
                    "time": {
                        "type": "number"
                    },
                    "start": {
                        "type": "string"
                    },
                    "end": {
                        "type": "string"
                    }
                },
                "required": ["value","time"]
            }
        }
    }
}